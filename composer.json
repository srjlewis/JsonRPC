{
    "name": "srjlewis/json-rpc",
    "description": "Simple Json-RPC client/server library that just works",
    "homepage": "https://github.com/srjlewis/JsonRPC",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric Guillot"
        },
        {
            "name": "Steven Lewis"
        }
    ],
    "replace": {
        "fguillot/json-rpc": "1.*"
    },
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "psr/log": "^2.0 || ^3.0"
    },
    "autoload": {
        "psr-0": {"JsonRPC": "src/"}
    },
    "require-dev": {
        "phpunit/phpunit": "^9 || ^10.0 || ^11.0"
    }
}
